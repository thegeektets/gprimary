	<queryString>
		<![CDATA[SELECT
     MIN(score.`math`) AS minscore_math,
     MIN(score.`kiswa`) AS minscore_kiswa,
     MIN(score.`eng`) AS minscore_eng,
     MIN(score.`science`) AS minscore_science,
     MIN(score.`socialstudies`) AS minscore_socialstudies,
     MAX(score.`math`) AS maxscore_math,
     MAX(score.`kiswa`) AS maxscore_kiswa,
     MAX(score.`eng`) AS maxscore_eng,
     MAX(score.`science`) AS maxscore_science,
     MAX(score.`socialstudies`) AS maxscore_socialstudies,
     AVG(score.`math`) AS avgscore_math,
     AVG(score.`kiswa`) AS avgscore_kiswa,
     AVG(score.`eng`) AS avgscore_eng,
     AVG(score.`science`) AS avgscore_science,
     AVG(score.`socialstudies`) AS avgscore_socialstudies,
       (if(score.`kiswa` is null,0,score.`kiswa`)+if(score.`eng` is null,0,score.`eng`)+if(score.`math` is null,0,score.`math`)+if(score.`science` is null,0,score.`science`)+if(score.`socialstudies` is null,0,score.`socialstudies`)) AS total,
     MIN((if(score.`kiswa` is null,0,score.`kiswa`)+if(score.`eng` is null,0,score.`eng`)+if(score.`math` is null,0,score.`math`)+if(score.`science` is null,0,score.`science`)+if(score.`socialstudies` is null,0,score.`socialstudies`))) AS mintotal,
     MAX((if(score.`kiswa` is null,0,score.`kiswa`)+if(score.`eng` is null,0,score.`eng`)+if(score.`math` is null,0,score.`math`)+if(score.`science` is null,0,score.`science`)+if(score.`socialstudies` is null,0,score.`socialstudies`))) AS maxtotal,
     AVG((if(score.`kiswa` is null,0,score.`kiswa`)+if(score.`eng` is null,0,score.`eng`)+if(score.`math` is null,0,score.`math`)+if(score.`science` is null,0,score.`science`)+if(score.`socialstudies` is null,0,score.`socialstudies`))) AS avgtotal,
     exam.`examtype` AS exam_examtype,
     exam.`class` AS exam_class,
     exam.`stream` AS exam_stream,
     exam.`term` AS exam_term,
     exam.`yearofadm` AS exam_yearofadm,
     exam.`examno` AS exam_examno,
     exam.`entry` AS exam_entry,
     school.`sname` AS school_sname,
     student.`studentnumber` AS student_studentnumber,
     student.`studentname` AS student_studentname,
     student.`stream` AS student_stream,
     student.`cclass` AS student_cclass,
     score.`admno` AS score_admno,
     score.`examno` AS score_examno,
     score.`math` AS score_math,
     score.`kiswa` AS score_kiswa,
     score.`eng` AS score_eng,
     score.`science` AS score_science,
     score.`socialstudies` AS score_socialstudies,
     subjectallocation_A.`teacherid` AS subjectallocation_A_teacherid,
     subjectallocation_A.`stream` AS subjectallocation_A_stream,
     subjectallocation_A.`class` AS subjectallocation_A_class,
     subjectallocation_A.`subject` AS subjectallocation_A_subject,
     subjectallocation_A.`key` AS subjectallocation_A_key,
     classallocation.`teacherid` AS classallocation_teacherid,
     classallocation.`year` AS classallocation_year,
     classallocation.`term` AS classallocation_term,
     classallocation.`total` AS classallocation_total,
     teachers.`idnumber` AS teachers_idnumber,
     teachers.`fullname` AS teachers_fullname,
     teachers.`sex` AS teachers_sex,
     teachers.`department` AS teachers_department,
     teachers.`responsibility` AS teachers_responsibility,
     teachers.`subject1` AS teachers_subject1,
     teachers.`subject2` AS teachers_subject2,
     teachers.`subject3` AS teachers_subject3,
     teachers.`subject4` AS teachers_subject4,
     teachers.`id` AS teachers_id
FROM
     `subjectallocation` subjectallocation_A INNER JOIN `exam` exam ON subjectallocation_A.`class` = exam.`class`
     AND exam.`stream` = subjectallocation_A.`stream`
     INNER JOIN `classallocation` classallocation ON subjectallocation_A.`teacherid` = classallocation.`teacherid`
     AND classallocation.`year` = exam.`yearofadm`
     AND exam.`term` = classallocation.`term`
     INNER JOIN `teachers` teachers ON classallocation.`teacherid` = teachers.`idnumber`
     AND subjectallocation_A.`teacherid` = teachers.`idnumber`
     INNER JOIN `score` score ON exam.`examno` = score.`examno`
     INNER JOIN `student` student ON exam.`class` = student.`cclass`
     AND student.`stream` = exam.`stream`
     AND student.`studentnumber` = score.`admno`,
     `school` school
WHERE
     exam.`examno` = $P{examno}]]>
	</queryString>
