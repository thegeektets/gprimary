	<queryString>
		<![CDATA[SELECT
     (if(score.`kiswa`!=0,score.`kiswa`,0)+if(score.`math`!=0,score.`math`,0)+if(score.`eng`!=0,score.`eng`,0)+if(score.`science`!=0,score.`science`,0)+if(score.`socialstudies`!=0,score.`socialstudies`,0)) AS total,
     exam.`examtype` AS exam_examtype,
     exam.`class` AS exam_class,
     exam.`stream` AS exam_stream,
     exam.`term` AS exam_term,
     exam.`yearofadm` AS exam_yearofadm,
     exam.`examno` AS exam_examno,
     exam.`entry` AS exam_entry,
     score.`admno` AS score_admno,
     score.`examno` AS score_examno,
     score.`math` AS score_math,
     score.`insha` AS score_insha,
     score.`lugha` AS score_lugha,
     score.`kiswa` AS score_kiswa,
     score.`comp` AS score_comp,
     score.`grammer` AS score_grammer,
     score.`eng` AS score_eng,
     score.`science` AS score_science,
     score.`re` AS score_re,
     score.`social` AS score_social,
     score.`socialstudies` AS score_socialstudies,
     student.`studentnumber` AS student_studentnumber,
     student.`studentname` AS student_studentname,
     student.`sex` AS student_sex,
     student.`stream` AS student_stream,
     student.`House` AS student_House,
     student.`DOA` AS student_DOA,
     student.`Class` AS student_Class,
     student.`ParentsId` AS student_ParentsId,
     student.`Bcertnumber` AS student_Bcertnumber,
     student.`spic` AS student_spic,
     student.`sport` AS student_sport,
     student.`kcpe` AS student_kcpe,
     student.`club` AS student_club,
     student.`society` AS student_society,
     student.`family` AS student_family,
     student.`cclass` AS student_cclass,
     student.`religion` AS student_religion,
     student.`dob` AS student_dob,
     subjectallocation.`teacherid` AS subjectallocation_teacherid,
     subjectallocation.`stream` AS subjectallocation_stream,
     subjectallocation.`class` AS subjectallocation_class,
     subjectallocation.`subject` AS subjectallocation_subject,
     subjectallocation.`key` AS subjectallocation_key,
     teachers.`idnumber` AS teachers_idnumber,
     teachers.`fullname` AS teachers_fullname,
     teachers.`sex` AS teachers_sex,
     teachers.`department` AS teachers_department,
     teachers.`responsibility` AS teachers_responsibility,
     teachers.`subject1` AS teachers_subject1,
     teachers.`subject2` AS teachers_subject2,
     teachers.`subject3` AS teachers_subject3,
     teachers.`subject4` AS teachers_subject4,
     teachers.`id` AS teachers_id,
     classallocation.`teacherid` AS classallocation_teacherid,
     classallocation.`year` AS classallocation_year,
     classallocation.`term` AS classallocation_term,
     classallocation.`total` AS classallocation_total,
     school.`sname` AS school_sname,
     school.`scpic` AS school_scpic,
     school.`address` AS school_address,
     school.`location` AS school_location
FROM
     `student` student INNER JOIN `score` score ON student.`studentnumber` = score.`admno`
     INNER JOIN `exam` exam ON score.`examno` = exam.`examno`
     AND student.`cclass` = exam.`class`
     INNER JOIN `classallocation` classallocation ON exam.`term` = classallocation.`term`
     AND exam.`yearofadm` = classallocation.`year`
     AND classallocation.`year` = exam.`yearofadm`
     INNER JOIN `subjectallocation` subjectallocation ON classallocation.`teacherid` = subjectallocation.`teacherid`
     AND student.`cclass` = subjectallocation.`class`
     AND student.`stream` = subjectallocation.`stream`
     INNER JOIN `teachers` teachers ON subjectallocation.`teacherid` = teachers.`idnumber`,
     `school` school
WHERE
     exam.`examno` = $P{examno}
     and student.`studentnumber` = $P{admno}
GROUP BY
     student.`studentnumber`
ORDER BY
     total DESC]]>
	</queryString>
